<% /* 文件切割器模版 */ %>
$.evalFile("<%=basedir %>/imgtool/cropcommons.jsx");

// ===========================
// 以下是与图片相关的部分
// ===========================
var opt = {
    fileName : '<%=filename %>',
    maxlevel : <%=maxlevel %>,
    minlevel : <%=minlevel %>,
    outFolder : '<%=outdir %>'
}

log('开始执行切分任务');
openfile(opt.fileName);
crop(opt);
closefile();
log('切分任务执行完成');